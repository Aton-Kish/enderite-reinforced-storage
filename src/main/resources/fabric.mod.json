{
  "schemaVersion": 1,
  "id": "enderitestorage",
  "version": "${version}",

  "name": "Enderite Reinforced Storage",
  "description": "Adds enderite reinforced storage.",
  "authors": ["Aton-Kish"],
  "contact": {
    "homepage": "https://github.com/Aton-Kish/enderite-reinforced-storage",
    "sources": "https://github.com/Aton-Kish/enderite-reinforced-storage",
    "issues": "https://github.com/Aton-Kish/enderite-reinforced-storage/issues"
  },

  "license": "MIT",
  "icon": "assets/enderitestorage/icon.png",

  "environment": "*",
  "entrypoints": {
    "reinfcore": [
      "atonkish.enderitestorage.entrypoint.reinfcore.ReinforcedCoreEntrypoint"
    ],
    "reinfchest": [
      "atonkish.enderitestorage.entrypoint.reinfchest.ReinforcedChestsEntrypoint"
    ],
    "reinfshulker": [
      "atonkish.enderitestorage.entrypoint.reinfshulker.ReinforcedShulkerBoxesEntrypoint"
    ],
    "reinfbarrel": [
      "atonkish.enderitestorage.entrypoint.reinfbarrel.ReinforcedBarrelsEntrypoint"
    ],
    "reinfcoreclient": [
      "atonkish.enderitestorage.entrypoint.reinfcore.ReinforcedCoreClientEntrypoint"
    ],
    "reinfchestclient": [
      "atonkish.enderitestorage.entrypoint.reinfchest.ReinforcedChestsClientEntrypoint"
    ],
    "reinfshulkerclient": [
      "atonkish.enderitestorage.entrypoint.reinfshulker.ReinforcedShulkerBoxesClientEntrypoint"
    ]
  },
  "mixins": ["enderitestorage.mixins.json"],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
